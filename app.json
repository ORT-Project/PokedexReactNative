{
  "expo": {
    "name": "PokedexReactNative",
    "slug": "beltadex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bluedyaltair.beltadex"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "assetBundlePatterns": [
      "**/*",
      "./assets/fonts/*"
    ],
    "scheme": "pokedex-react-native",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "515d7209-cd77-456c-ac1b-c59367e0004a"
      }
    },
    "owner": "bluedyaltair"
  }
}
